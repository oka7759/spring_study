{{>layout/header}}
<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>

    </tr>
    </thead>
    <tbody>
    {{#articleList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/article/{{id}}">{{title}}</a> </td>
            <td>{{content}}</td>

        </tr>
    {{/articleList}}

    </tbody>
</table>
<a class="btn btn-primary" href="/article/new">New article</a>


{{>layout/footer}}
